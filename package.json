    "commit-and-tag-version": "^12.0.0",
    "dotenv": "^16.3.1",
    "eslint": "9.15.0",
    "eslint-plugin-jest": "^28.8.3",
    "globals": "15.9.0",
    "jest": "^29.5.0",
    "superagent": "10.1.0",
