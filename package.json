{
  "name": "@certible/superagent-aws-sign",
  "type": "module",
  "version": "3.0.0",
  "description": "Superagent plugin to sign requests using AWS Signature Version 4",
  "author": "Certible <oberreiter@certible.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/certible/superagent-aws-sign.git"
  },
  "bugs": {
    "url": "https://github.com/certible/superagent-aws-sign/issues"
  },
  "keywords": [
    "superagent",
    "aws",
    "request"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs"
    }
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "files": [
    "index.cjs",
    "index.d.ts",
    "index.mjs"
  ],
  "scripts": {
    "build": "tsup && npm run types",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "types": "tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir .",
    "jest:vm": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js",
    "test": "npm run lint && npm run jest:vm",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.359.0",
    "@aws-sdk/credential-providers": "^3.359.0",
    "aws4": "^1.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "6.1.0",
    "dotenv": "^17.1.0",
    "eslint": "^9.15.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "jest": "^30.0.4",
    "superagent": "^10.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.2"
  }
}
