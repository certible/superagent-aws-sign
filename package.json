{
  "name": "@certible/superagent-aws-sign",
  "type": "module",
  "version": "3.0.0",
  "description": "Superagent plugin to sign requests using AWS Signature Version 4",
  "author": "Certible <oberreiter@certible.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/certible/superagent-aws-sign.git"
  },
  "bugs": {
    "url": "https://github.com/certible/superagent-aws-sign/issues"
  },
  "keywords": [
    "superagent",
    "aws",
    "request"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.cjs",
      "default": "./index.mjs"
    }
  },
  "main": "index.cjs",
  "module": "index.mjs",
  "files": [
    "index.cjs",
    "index.d.ts",
    "index.d.ts.map",
    "index.mjs"
  ],
  "scripts": {
    "build": "npm run build:cjs && npm run build:esm && npm run types",
    "build:cjs": "cp src/index.js index.cjs",
    "build:esm": "node scripts/build-esm.cjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "commit-and-tag-version",
    "types": "tsc src/index.js --declaration --allowJs --emitDeclarationOnly --outDir .",
    "test": "jest"
  },
  "commit-and-tag-version": {
    "scripts": {
      "prerelease": "npm run build"
    }
  },
  "dependencies": {
    "@aws-sdk/client-sts": "^3.359.0",
    "@aws-sdk/credential-providers": "^3.359.0",
    "aws4": "^1.12.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.13.2",
    "commit-and-tag-version": "^12.0.0",
    "dotenv": "17.1.0",
    "eslint": "^9.15.0",
    "globals": "^16.2.0",
    "jest": "30.0.4",
    "superagent": "^10.1.0",
    "typescript": "^5.3.2"
  }
}
